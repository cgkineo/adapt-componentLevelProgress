{{#if _showComponentCompletion}}
<div class="{{_component}}-title component-title">
    <div role="heading" tabindex="0" class="{{_component}}-title-inner component-title-inner" aria-label="{{titleAriaLabel}}" aria-label="4"></div>
	<div class="component-completion {{#if _isComplete}}complete {{else}}incomplete {{/if}}"></div>
	{{#if displayTitle}}
	<div class="component-title-text a11y-ignore" aria-hidden="true" tabindex="-1">
    {{{compile displayTitle this}}}
    </div>
    {{/if}}
    
</div>
{{else if displayTitle}}
<div class="{{_component}}-title component-title">
    <div role="heading" tabindex="0" class="{{_component}}-title-inner component-title-inner" aria-level="4">
        {{{compile displayTitle this}}}
    </div>
</div>
{{/if}}